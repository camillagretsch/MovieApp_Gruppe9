<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
   <servlet>
    <servlet-name>dataImportServiceImpl</servlet-name>
    <servlet-class>com.SE.gruppe9.server.DataImportServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>dataImportServiceImpl</servlet-name>
    <url-pattern>/movieapp_gruppe9/datas</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
                <servlet-name>gwtTableToExcelServlet</servlet-name>
                <servlet-class>com.googlecode.gwtTableToExcel.server.TableToExcelServlet</servlet-class>
   </servlet>

   <servlet-mapping>
           <servlet-name>gwtTableToExcelServlet</servlet-name>
           <url-pattern>/movieapp_gruppe9/excel</url-pattern>
   </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>MovieApp_Gruppe9.html</welcome-file>
  </welcome-file-list>

</web-app>
